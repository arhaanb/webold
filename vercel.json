{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=60, stale-while-revalidate"
        }
      ]
    },
    {
      "source": "/api/spotify",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=10, stale-while-revalidate=10"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/sahyog/app",
      "destination": "https://sahyog-dash.herokuapp.com"
    },
    {
      "source": "/sahyog/apk",
      "destination": "https://sahyog.arhaanb.co/sahyog.apk"
    }
  ],
  "rewrites": [
    {
      "source": "/:uid",
      "destination": "/api/redirect"
    },
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/gh/(.*)", "destination": "/api" },
    { "source": "/poopy/(.*)", "destination": "/api" }
  ],
  "github": {
    "autoAlias": true,
    "enabled": true,
    "silent": true
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "public": true
}
