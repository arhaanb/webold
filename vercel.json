{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=60, stale-while-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/gh",
      "destination": "https://github.com/arhaanb"
    }
  ],
  "rewrites": [
    {
      "source": "/:uid",
      "destination": "/api/redirect"
    }
  ],
  "github": {
    "autoAlias": true,
    "enabled": true
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "public": true
}
